        "ng2-smart-table": "1.0.3",
        "ng2-tree": "2.0.0-alpha.5",
        "ngx-uploader": "2.2.5",
        "normalize.css": "6.0.0",
        "roboto-fontface": "0.7.0",
        "rxjs": "^5.4.3",
        "zone.js": "^0.8.18"
    },
    "devDependencies": {
        "@angular/cli": "1.4.4",
