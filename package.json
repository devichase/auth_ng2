        "@types/google-maps": "^3.2.0",
        "@types/hammerjs": "^2.0.35",
        "@types/jasmine": "2.6.0",
        "@types/jquery": "3.2.12",
        "@types/jquery.slimscroll": "1.3.31",
        "@types/lodash": "4.14.76",
        "@types/node": "~8.0.31",
        "@types/arcgis-js-api": "^4.4.0",
        "codelyzer": "~3.2.0",
        "gh-pages": "1.0.0",
